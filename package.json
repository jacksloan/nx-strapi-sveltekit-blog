  "scripts": {
    "start": "nx serve",
    "start:api": "env-cmd npx nx serve api",
    "openapi": "openapi-generator-cli",
    "start:web": "env-cmd npx nx serve frontend",
    "fix-codegen-json": "sed 's/\"=\"/\"_eq\"/g' ./apps/api/extensions/documentation/documentation/1.0.0/full_documentation.json > ./apps/api/extensions/documentation/documentation/1.0.0/full_documentation_fixed.json",
    "codegen": "npm run fix-codegen-json && swagger-nodegen-cli generate -i ./apps/api/extensions/documentation/documentation/1.0.0/full_documentation_fixed.json -o libs/api-client/src/lib -l typescript-fetch",
    "build": "nx build",
    "test": "nx test"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/angular": "^13.2.2",
    "@nrwl/cli": "13.2.2",
    "@nrwl/eslint-plugin-nx": "13.2.2",
    "@nrwl/jest": "13.2.2",
    "@nrwl/linter": "13.2.2",
    "@nrwl/tao": "13.2.2",
    "@nrwl/workspace": "13.2.2",
    "@nx-extend/strapi": "^2.0.1",
    "@nxext/sveltekit": "^12.0.0-next.1",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@types/jest": "27.0.2",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3",
    "vite": "^2.1.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "env-cmd": "^10.1.0",
    "portable-fetch": "^3.0.0",
    "tslib": "^2.0.0"
  }
}
