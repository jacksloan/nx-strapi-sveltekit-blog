{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "nx-strapi-api-codegen",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "openapi-json": {
      "type": "string",
      "description": "Path to openapi spec file",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": ["name", "openapi-json"]
}
