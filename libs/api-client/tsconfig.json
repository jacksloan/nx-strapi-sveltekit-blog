{
  "extends": "../../tsconfig.base.json",

  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "../../dist/out-tsc",
    "target": "es2017",
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "sourceMap": true,
    "types": ["node"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": ["src/lib/index.ts"],
  "include": []
}
